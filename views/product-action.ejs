<div class="product-actions-wrapper">
    <div class="product-actions">


      <form class="variations_form cart" method="post">

            <table >
                <tbody>
                    <tr>
                        <td ><img src="../../images/brands/fk-logo.jpg" data-echo="../../images/brands/fk-logo.jpg" class="img-responsive" alt="" width="120" height="75"></td>
                        <td style="font-weight: bold;"><%=result.title.flipkart_title %></td>
                        <td  style="font-weight: bold;"><del>&#8377;<%=result.listprice.flipkart_listprice %></del></td>
                        <td style="font-weight: bold;">&#8377;<%=result.offerprice.flipkart_offerprice %></td>
                        <td><a href=<%=result.url.flipkart_url %> ><button type="submit" class="affiliatebutton">View</button></a></td>
                    </tr>
                    <tr>
                        <td ><img src="../../images/brands/amazon-logo.jpg" data-echo="../../images/brands/amazon-logo.jpg" class="img-responsive" alt="" width="120" height="75"></td>
                        <td style="font-weight: bold;"><%=result.title.amazon_title %></td>
                        <td style="font-weight: bold;"><del>&#8377;<%=result.listprice.amazon_listprice %></del></td>
                        <td style="font-weight: bold;">&#8377;<%=result.offerprice.amazon_offerprice %></td>
                        <td><a href=<%=result.url.amazon_url %>><button type="submit" class="affiliatebutton">View</button></a></td>
                    </tr>
                </tbody>
            </table>

            <div class="single_variation_wrap">
                <div class="woocommerce-variation single_variation"></div>
                <div class="woocommerce-variation-add-to-cart variations_button">


                    <input type="hidden" name="add-to-cart" value="2439" />
                    <input type="hidden" name="product_id" value="2439" />
                    <input type="hidden" name="variation_id" class="variation_id" value="0" />
                </div>
            </div>
        </form>


    </div>
</div>
